{
    "version": 3,
    "builds": [
        { "src": "startGanache.sh", "use": "@vercel/node" },
        { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/server", "dest": "server.js" },
        { "src": "/startGanache", "dest": "startGanache.sh" }
    ],
    "functions": {
        "startGanache": { "runtime": "bash", "maxDuration": 0 }
    }
}
